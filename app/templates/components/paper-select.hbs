{{#paper-menu-toggle}}
  <span>{{if selection selection prompt}}</span>
  <div class="md-select-icon"></div>
{{/paper-menu-toggle}}
{{#if isOpen}}
  {{#paper-menu}}
    {{#each item in content}}
      {{#paper-menu-item action="select" param=item}}
        <div class="md-text">{{item}}</div>
      {{/paper-menu-item}}
    {{/each}}
  {{/paper-menu}}
{{/if}}
