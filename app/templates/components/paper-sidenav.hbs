{{#unless closed}}
  {{paper-backdrop
    locked-open=isLockedOpen
    opaque=true
    class="md-sidenav-backdrop"
    onTap=(action "onBackdropTap")
  }}
{{/unless}}

{{#paper-sidenav-inner
  class=class
  name=name
  position=position
  lockedOpen=lockedOpen
  closed=closed
  closeOnClick=closeOnClick
  onToggle=(action "onToggle")}}
  {{yield}}
{{/paper-sidenav-inner}}
